{
    "manifest_version": 3,
    "name": "LC Friends Live Contest Ranking",
    "version": "1.1.1",
    "description": "A simple Chrome extension example",
    "action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "storage"
    ]
}
